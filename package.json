{
  "name": "camino-trekker",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "cert": "rm -rf .cert && mkdir -p .cert && mkcert -key-file ./.cert/key.pem -cert-file ./.cert/cert.pem 'localhost' 127.0.0.1 `ipconfig getifaddr en0`",
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test:watch": "npm run test -- --watch",
    "storybook:https": "npm run storybook -- --https --ssl-cert ./.cert/cert.pem --ssl-key ./.cert/key.pem",
    "gh-pages:build": "vite build",
    "gh-pages:publish": "gh-pages --dist dist",
    "gh-pages": "npm run gh-pages:build && npm run gh-pages:publish"
  },
  "dependencies": {
    "@ar-js-org/ar.js": "^3.4.0-alpha-rc2",
    "@fontsource/material-icons": "^4.5.4",
    "@fontsource/poppins": "^4.5.5",
    "@types/jest": "^27.4.1",
    "@vueuse/core": "^8.2.5",
    "aframe": "^1.3.0",
    "aframe-look-at-component": "^1.0.0",
    "axios": "^0.26.1",
    "dompurify": "^2.3.6",
    "mapbox-gl": "^2.8.0",
    "marked": "^4.0.14",
    "md5": "^2.3.0",
    "normalize.css": "^8.0.1",
    "vue": "^3.2.31",
    "vue-easy-lightbox": "^1.4.1",
    "vue-router": "^4.0.14",
    "vue-types": "^4.1.1",
    "vuex": "^4.0.2",
    "vuex-router-sync": "^6.0.0-rc.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@storybook/addon-essentials": "^6.4.20",
    "@storybook/builder-vite": "^0.1.29",
    "@storybook/vue3": "^6.4.22",
    "@types/aframe": "^1.2.0",
    "@types/three": "^0.139.0",
    "@types/tweenjs": "^1.0.4",
    "@vitejs/plugin-vue": "^2.3.1",
    "babel-loader": "^8.2.4",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.5.11",
    "eslint-plugin-vue": "^8.6.0",
    "gh-pages": "^3.2.3",
    "jest": "^27.5.1",
    "react": "^17.0.2",
    "vite": "^2.9.1",
    "vite-plugin-eslint": "^1.3.0",
    "vue-loader": "^17.0.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ]
}
